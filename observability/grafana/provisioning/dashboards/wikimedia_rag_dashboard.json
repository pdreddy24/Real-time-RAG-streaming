      "type": "timeseries",
      "title": "RAG API - p95 latency (sec)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(rag_api_request_latency_seconds_bucket[5m])) by (le, path))",
          "legendFormat": "p95 {{path}}"
        }
      ]
    },
    {
      "type": "timeseries",
