{
  "uid": "wikimedia-rag-flow",
  "title": "Wikimedia RAG Flow Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Processed Rate (events/sec)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(rag_indexer_processed_total[5m]))", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "Consumer Lag (rag-indexer-v3)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(kafka_consumergroup_lag{consumergroup=\"rag-indexer-v3\"}) by (topic)",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Error Rate (errors/sec)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        { "expr": "sum(rate(rag_indexer_errors_total[5m]))", "refId": "A" }
      ]
    },
    {
      "type": "timeseries",
      "title": "P95 Processing Latency (seconds)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(rag_indexer_processing_latency_seconds_bucket[5m])) by (le))",
          "refId": "A"
        }
      ]
    }
  ]
}
