{
  "uid": "wikimedia-rag-flow",
  "title": "Wikimedia RAG Flow Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "tags": ["wikimedia", "rag", "kafka", "observability"],
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Processed Rate (events/sec)",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rag_indexer_processed_total[5m]))"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Consumer Lag (max)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "max(kafka_consumergroup_lag{consumergroup=\"rag-indexer-v3\", topic=\"wikimedia.cleaned\"})"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Error Rate (%)",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (sum(rate(rag_indexer_errors_total[5m])) / clamp_min(sum(rate(rag_indexer_processed_total[5m])), 1e-9))"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "p95 Processing Latency (seconds)",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum(rate(rag_indexer_processing_latency_seconds_bucket[5m])) by (le))"
        }
      ]
    }
  ]
}
